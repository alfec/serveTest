{
  "name": "serveTest",
  "version": "1.0.0",
  "description": "simple project with UI and API test",
  "scripts": {
    "cy:open": "cypress open",
    "test": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports/mochawesome",
    "test:ui": "cypress run --spec \"cypress/e2e/ui/**\"",
    "test:api": "cypress run --spec \"cypress/e2e/api/**\""
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "cypress": "^12.0.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-xpath": "^2.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "prettier": "^3.6.2"
  }
}
